{{>layouts/header}}

{{! 리스트 출력 }}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Title</th>
            <th scope="col">Content</th>
        </tr>
    </thead>
    
    <tbody>
        {{#articlelist}}
        {{! 모델에 등록한 데이터 이름: article }}
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
        {{/articlelist}}
    </tbody>
</table>
{{>layouts/footer}}
